const prompt = require('prompt-sync')();

function myComp(p1,p2)
{
    if (p1 > p2)
        return p1;
    if (p2 > p1)
        return p2;
    else
        return 0;
}


function getComp()
{
    let s1=parseFloat(prompt("Anna 1. luku: "));
    let s2=parseFloat(prompt("Anna 2. luku: "));
    console.log('Suurempi arvo on', myComp(s1, s2));
    return(myComp(s1,s2));
}

getComp();